<?xml version="1.0" encoding="UTF-8"?>
<config name="ops_transformer" copy_type="delivery" src_path="lib/" dst_path="ops_transformer" install_type="all" install_mod="550" install_own="$username:$usergroup">
    <package_info>
        <product_name>cann</product_name>
        <chip_name>910b</chip_name>
        <func_name>ops-transformer</func_name>
        <suffix>run</suffix>
        <install_script>ops_transformer/script/install.sh</install_script>
        <help>ops_transformer/script/help.info</help>
        <cleanup>ops_transformer/script/cleanup.sh</cleanup>
        <parallel>true</parallel>
        <check_features>true</check_features>
    </package_info>
    <generate_info value="ops_transformer_version.h" dst_path="ops_transformer" install_path="$(TARGET_ENV)/include/version" install_mod="440" install_type="devel" generator="version_header">
       <OPS_TRANSFORMER_VERSION>$(VERSION_DIR)</OPS_TRANSFORMER_VERSION>
       <OPS_TRANSFORMER_TIMESTAMP>$(TIMESTAMP_NO)</OPS_TRANSFORMER_TIMESTAMP>
    </generate_info>
    <generate_info value="scene.info" dst_path="ops_transformer" install_path="ops_transformer" install_mod="440">
        <os>$(OS_NAME)</os>
        <os_version>$(OS_VER)</os_version>
        <arch>$(ARCH)</arch>
    </generate_info>
    <file_info value="ops_transformer_script" copy_type="source" src_path="" dst_path="ops_transformer" install_path="ops_transformer">
        <file value="script" entity="true"/>
        <file value="bin" entity="true"/>
        <file value="version.info" dst_path="."/>
    </file_info>
    <block_info dependtree="false" dst_path="ops_transformer" block_conf_path="ascend">
        <block name="EngineeringCommon"/>
        <block name="EngineeringFiles"/>
        <block name="OpsTransformer"/>
        <block name="OpsTransformerInc"/>
    </block_info>
    <dir_info value="ops_transformer" install_path="ops_transformer" install_type="all">
        <path value="ops_transformer/script" install_mod="555"/>
        <path value="ops_transformer/bin" install_mod="555"/>
        <path value="ops_transformer/lib64" install_mod="555"/>
        <path value="ops_transformer/include" install_mod="555"/>
    </dir_info>
</config>
